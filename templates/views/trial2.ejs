    <style>
        .centered {
            position: absolute;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
        }

        .container .box {
            display: flex;
            flex-direction: row;

        }


        .container .box .box-cell.box1 {
            background: green;
            color: white;
            text-align: justify;
        }

        .container .box .box-cell.box2 {
            background: lightgreen;
            text-align: justify
        }

        img {
            width: 100%;
            height: 100%;
        }

        .info {
            padding: 0px 50px;
        }

        .session h1 {
            position: relative;
            display: inline-block;
            font-family: "Playfair Display";
            font-size: 40px;
            margin-bottom: 3rem !important;
        }

        .section-heading h1:before {
            width: 100px;
            bottom: -10px;
            left: 50%;
        }

       
    </style>
